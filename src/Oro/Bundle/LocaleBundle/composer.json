{
    "name": "oro/locale-bundle",
    "type": "symfony-bundle",
    "description": "BAP Locale Bundle",
    "keywords": ["BAP", "Localization"],
    "homepage": "https://github.com/laboro/LocaleBundle",
    "license": "MIT",
    "require": {
        "php": "7.1.*",
        "ext-intl": "*",
        "symfony/symfony": "^3.4",
        "oro/config": "dev-master",
        "oro/requirejs-bundle": "dev-master",
        "oro/config-bundle": "dev-master",
        "bower-asset/moment": "2.18.*",
        "bower-asset/moment-timezone": "0.5.*",
        "bower-asset/numeral": "2.0.6"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\LocaleBundle": "" }
    },
    "target-dir": "Oro/Bundle/LocaleBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
