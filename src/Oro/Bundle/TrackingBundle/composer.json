    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": ">=2.3",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/data-fixtures": "@dev",
        "doctrine/doctrine-fixtures-bundle": "@dev",
        "friendsofsymfony/rest-bundle": "1.5.0-RC2",
        "nelmio/api-doc-bundle": "dev-master",
        "sensio/framework-extra-bundle": "2.3.*",
        "oro/cron-bundle": "@dev",
        "oro/importexport-bundle": "@dev",
        "oro/ui-bundle": "@dev",
        "oro/security-bundle": "@dev",
        "oro/soap-bundle": "@dev",
        "oro/config-bundle": "@dev",
        "oro/entity-config-bundle": "@dev",
        "oro/user-bundle": "@dev",
        "oro/batch-bundle": "@dev",
        "oro/migration-bundle": "@dev",
        "oro/testframework-bundle": "@dev",
