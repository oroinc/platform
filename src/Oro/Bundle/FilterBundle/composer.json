{
    "name": "oro/filter-bundle",
    "type": "symfony-bundle",
    "description": "BAP Filter Bundle",
    "keywords": ["BAP", "UI", "User Interface", "Filter"],
    "homepage": "https://github.com/laboro/FilterBundle",
    "license": "MIT",
    "require": {
        "php": "7.1.*",
        "symfony/symfony": "^3.4",
        "oro/ui-bundle": "dev-master",
        "oro/requirejs-bundle": "dev-master",
        "oro/locale-bundle": "dev-master",
        "luxifer/doctrine-functions": "1.2.*",
        "nesbot/Carbon": "1.8.*"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\FilterBundle": "" }
    },
    "target-dir": "Oro/Bundle/FilterBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
