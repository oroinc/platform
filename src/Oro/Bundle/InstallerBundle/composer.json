{
    "name": "oro/installer-bundle",
    "type": "symfony-bundle",
    "description": "Web installer for BAP/OroCRM",
    "keywords": ["BAP"],
    "homepage": "https://github.com/laboro/InstallerBundle",
    "license": "MIT",
    "require": {
        "php": "7.1.*",
        "oro/config": "dev-master",
        "oro/cache-bundle": "dev-master",
        "components/jquery": "2.1.*",
        "bower-asset/jquery-ajax-queue": "0.0.1",
        "symfony/form": "2.8.*",
        "symfony/validator": "2.8.*"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\InstallerBundle": "" }
    },
    "target-dir": "Oro/Bundle/InstallerBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
