{
    "name": "oro/imap-bundle",
    "type": "symfony-bundle",
    "description": "BAP Imap Bundle",
    "keywords": ["BAP"],
    "homepage": "https://github.com/laboro/ImapBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "2.8.*, !=2.8.10",
        "oro/log": "dev-master",
        "zendframework/zend-mail": "2.1.5",
        "oro/platform-bundle": "dev-master",
        "oro/email-bundle": "dev-master",
        "oro/google-integration-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\ImapBundle": "" }
    },
    "target-dir": "Oro/Bundle/ImapBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
