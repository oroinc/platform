# Configuration for https://github.com/oroinc/phpstan-rules/
trusted_data:
    properties:
        Oro\Bundle\PlatformBundle\MaterializedView\MaterializedViewRepository:
            createQueryBuilder:
                materializedViewName: true

    variables:
        Oro\Bundle\PlatformBundle\Entity\Repository\NumberSequenceRepository:
            upsertSequence:
                sql: true

    check_methods:
        Oro\Bundle\PlatformBundle\Entity\Repository\NumberSequenceRepository:
            upsertSequence: [4]
