{
    "name": "oro/integration-bundle",
    "type": "symfony-bundle",
    "description": "BAP Integration Bundle",
    "keywords": ["BAP", "Integration"],
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": ">=2.3,<2.4-dev",
        "oro/log": "dev-master",
        "oro/workflow-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Oro\\Bundle\\IntegrationBundle": ""
        }
    },
    "target-dir": "Oro/Bundle/IntegrationBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
