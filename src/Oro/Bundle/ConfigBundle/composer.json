{
    "name": "oro/config-bundle",
    "type": "symfony-bundle",
    "description": "BAP Config Bundle",
    "keywords": ["BAP"],
    "homepage": "https://github.com/laboro/ConfigBundle",
    "license": "MIT",
    "require": {
        "php": "7.1.*",
        "symfony/symfony": "^3.4",
        "oro/config": "dev-master",
        "oro/ui-bundle": "dev-master",
        "oro/user-bundle": "dev-master",
        "oro/requirejs-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Oro\\Bundle\\ConfigBundle": ""
        }
    },
    "target-dir": "Oro/Bundle/ConfigBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
