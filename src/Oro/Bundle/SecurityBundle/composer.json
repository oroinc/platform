{
    "name": "oro/security-bundle",
    "type": "symfony-bundle",
    "description": "BAP Security Bundle",
    "keywords": ["BAP"],
    "license": "MIT",
    "require": {
        "php": "7.1.*",
        "symfony/symfony": "^3.4",
        "ass/xmlsecurity": "dev-master",
        "oro/config": "dev-master",
        "oro/config-expression": "dev-master",
        "oro/entity-bundle": "dev-master",
        "oro/entity-config-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\SecurityBundle": "" }
    },
    "target-dir": "Oro/Bundle/SecurityBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
