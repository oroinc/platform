{
    "name": "oro/theme-bundle",
    "type": "symfony-bundle",
    "description": "BAP Theme Bundle",
    "keywords": ["BAP", "Theme"],
    "homepage": "https://github.com/laboro/ThemeBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "2.8.*, !=2.8.10",
        "oro/assetic-bundle": "@dev"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\ThemeBundle": "" }
    },
    "target-dir": "Oro/Bundle/ThemeBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
