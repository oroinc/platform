{
    "name": "oro/requirejs-bundle",
    "type": "symfony-bundle",
    "description": "BAP RequireJS Bundle",
    "keywords": ["BAP", "RequireJS"],
    "homepage": "https://github.com/orocrm/RequireJSBundle",
    "license": "MIT",
    "require": {
        "php": "7.1.*",
        "symfony/symfony": "^3.4",
        "doctrine/doctrine-cache-bundle": "1.3.*",
        "npm-asset/requirejs": "2.3.*"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\RequireJSBundle": "" }
    },
    "target-dir": "Oro/Bundle/RequireJSBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
