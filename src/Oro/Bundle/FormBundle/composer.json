{
    "name": "oro/form-bundle",
    "type": "symfony-bundle",
    "description": "BAP Form Bundle",
    "keywords": [
        "BAP",
        "Form"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "2.8.*, !=2.8.10",
        "genemu/form-bundle": "2.1.*",
        "oro/security-bundle": "dev-master",
        "oro/search-bundle": "dev-master",
        "oro/user-bundle": "dev-master",
        "oro/requirejs-bundle": "dev-master",
        "oro/locale-bundle": "dev-master",
        "oro/ui-bundle": "dev-master",
        "oro/entity-config-bundle": "dev-master",
        "oro/form-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": {"Oro\\Bundle\\FormBundle": ""}
    },
    "target-dir": "Oro/Bundle/FormBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
