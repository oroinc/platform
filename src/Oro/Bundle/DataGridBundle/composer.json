{
    "name": "oro/datagrid-bundle",
    "type": "symfony-bundle",
    "description": "BAP DataGrid Bundle",
    "keywords": ["BAP", "DataGrid"],
    "license": "MIT",
    "require": {
        "php": "7.1.*",
        "symfony/symfony": "^3.4",
        "oro/config": "dev-master",
        "oro/ui-bundle": "dev-master",
        "oro/requirejs-bundle": "dev-master",
        "oro/security-bundle": "dev-master",
        "bower-asset/backgrid": "0.3.7",
        "bower-asset/backgrid-paginator": "0.3.8",
        "bower-asset/backbone-pageable": "1.2.3"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\DataGridBundle": "" }
    },
    "target-dir": "Oro/Bundle/DataGridBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
