{
    "name": "oro/cron-bundle",
    "type": "symfony-bundle",
    "description": "BAP Cron Bundle",
    "keywords": ["BAP", "cron"],
    "homepage": "https://github.com/laboro/CronBundle",
    "license": "MIT",
    "require": {
        "php": "7.1.*",
        "symfony/symfony": "^3.4",
        "mtdowling/cron-expression": "1.0.*",
        "oro/log": "dev-master",
        "oro/ui-bundle": "dev-master",
        "oro/user-bundle": "dev-master",
        "oro/navigation-bundle": "dev-master",
        "oro/translation-bundle": "dev-master",
        "oro/platform-bundle": "dev-master",
        "oro/requirejs-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\CronBundle": "" }
    },
    "target-dir": "Oro/Bundle/CronBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
